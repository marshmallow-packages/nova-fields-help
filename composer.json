{
    "name": "marshmallow/nova-fields-help",
    "description": "A Laravel Nova help field.",
    "keywords": [
        "laravel",
        "nova",
        "marshmallow",
        "help"
    ],
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev"
        },
        {
            "name": "Alessandro Benoit",
            "email": "alessandro@comodolab.it"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Nova\\Fields\\Help\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Nova\\Fields\\Help\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
